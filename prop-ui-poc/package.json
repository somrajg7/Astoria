{
  "name": "prop-ui-poc",
  "private": true,
  "version": "0.0.1",
  "description": "Prop UI Poc App",
  "main": "",
  "scripts": {
    "clean": "rimraf dist",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist/app && copyfiles -f ./src/assets/images/** ./dist/app/assets/images",
    "dist": "npm run clean && npm run dist:messages && npm run copy && webpack --env=dist && npm run dist:targz",
    "dist:messages": "buildMessages --i=src/cbdata/en-US__messages__poc.json --c=node_modules/@prop-ui/common/cbdata/en-US__messages__common.json --o=dist/cbdata/docs/en-US%3a%3amessages%3a%3apoc.json",
    "dist:targz": "targz compress ./dist/app ./dist/app.tar.gz && targz compress ./dist/cbdata ./dist/cbdata.tar.gz",
    "link": "npm link @prop-ui/common && npm install && npm install",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "start": "npm run dist:messages && node server.js --env=dev",
    "start:dist": "npm run dist && node server.js --env=dist",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
